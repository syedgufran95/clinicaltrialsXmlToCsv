  <primary_outcome>
    <measure>Correlation between serology testing and clinical suspicion of COVID-19</measure>
    <time_frame>Serology Test #1 (Study Enrollment)</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Prevalence of SARS-CoV2 exposure</measure>
    <time_frame>Serology Tests #1 and 2 (3 months apart)</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Correlation of pre-existing risk factors with results of serology testing</measure>
    <time_frame>Serology Test #1 (Study Enrollment)</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of asymptomatic participants with positive antibody serology</measure>
    <time_frame>Serology Test #1 (Study Enrollment)</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportion of antibody- positive participants who subsequently develop COVID-19</measure>
    <time_frame>Serology Tests #1 (Study Enrollment) and Serology Test #2 (3 months apart)</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportion of participants who are eligible for in plasma donation</measure>
    <time_frame>Serology Tests #1 and 2 (3 months apart)</time_frame>
  </secondary_outcome>
  <enrollment type="Anticipated">300</enrollment>
  <condition>COVID</condition>
  <intervention>
    <intervention_type>Diagnostic Test</intervention_type>
    <intervention_name>SARS-CoV2 serum antibody testing</intervention_name>
    <description>Participants will have SARS-CoV2 serum antibody testing performed twice within 4 months of each other.</description>
  </intervention>
