  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <intervention_model_description>Patients will be randomized 1:1:1 to a single IV dose of ATYR1923 1 mg/kg, ATYR1923 3 mg/kg, or placebo.</intervention_model_description>
