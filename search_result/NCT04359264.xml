  <brief_title>Direct Income SupporT and Advice Negating Spread of Epidemic COVID-19: a Randomized Controlled Trial</brief_title>
  <acronym>DISTANSE COVID</acronym>
  <official_title>Direct Income SupporT and Advice Negating Spread of Epidemic COVID-19: a Randomized Controlled Trial (DISTANSE COVID-19 RCT)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>St. Michael's Hospital, Toronto</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>St. Michael's Hospital, Toronto</source>
  <oversight_info>
    <has_dmc>No</has_dmc>
    <is_fda_regulated_drug>No</is_fda_regulated_drug>
    <is_fda_regulated_device>No</is_fda_regulated_device>
  </oversight_info>
  <brief_summary>
    <textblock>
      This is a clinical trial of a cash transfer on symptoms consistent with COVID-19 and
      adherence to physical distancing guidance.
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date type="Actual">April 20, 2020</start_date>
  <completion_date type="Anticipated">December 30, 2020</completion_date>
  <primary_completion_date type="Actual">May 29, 2020</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Interventional</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <allocation>Randomized</allocation>
    <intervention_model>Parallel Assignment</intervention_model>
    <primary_purpose>Prevention</primary_purpose>
    <masking>Double (Investigator, Outcomes Assessor)</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Symptoms consisent with COVID-19</measure>
    <time_frame>14 days</time_frame>
    <description>32-item Influenza Patient-reported Outcome (FLU-PRO) symptom score. Higher score is worse.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of close contacts</measure>
    <time_frame>Sum of contacts on days 5, 10 and 14</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Number of participants with positive test for COVID-19</measure>
    <time_frame>14 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Self-reported health based on the single question: &quot;In general, would you say your health is..&quot;</measure>
    <time_frame>14 days</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Ability to make ends meet based on the single question: &quot;At the end of the month, are you able to make ends meet?&quot;</measure>
    <time_frame>14 days</time_frame>
  </secondary_outcome>
  <other_outcome>
    <measure>Health care utilization</measure>
    <time_frame>14 days</time_frame>
  </other_outcome>
  <other_outcome>
    <measure>Food security</measure>
    <time_frame>14 days</time_frame>
    <description>Three-items (worry about food running out, food did not last, balanced meals)</description>
  </other_outcome>
  <other_outcome>
    <measure>Cost-related medicine non-adherence</measure>
    <time_frame>14 days</time_frame>
    <description>Single item: &quot;During the last two weeks, did you or any member of your household not take a medication, skip some doses, split pills or do anything else to make a medication last longer due to the cost?&quot;</description>
  </other_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">392</enrollment>
  <condition>COVID-19</condition>
  <arm_group>
    <arm_group_label>Cash transfer</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <arm_group>
    <arm_group_label>Control</arm_group_label>
    <arm_group_type>No Intervention</arm_group_type>
  </arm_group>
  <intervention>
    <intervention_type>Other</intervention_type>
    <intervention_name>Cash transfer</intervention_name>
    <description>Cash transfer of $1000</description>
    <arm_group_label>Cash transfer</arm_group_label>
  </intervention>
  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  trouble &quot;making ends meet at the end of the month&quot; due to COVID-19 related disruptions

        Exclusion Criteria:

          -  confirmed positive for COVID-19 based on laboratory testing

          -  current symptoms consistent with COVID-19
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
