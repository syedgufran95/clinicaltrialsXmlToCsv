  <eligibility>
    <study_pop>
      <textblock>
        COVID-19 patients
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients admitted to hospital with biological samples positive for SARS-CoV-2

          -  Patients admitted to hospital with negative test but clinical and radiological
             characteristics highly suggestive of SARS-CoV-2 disease

          -  Patients discharged from emergency department with biological samples positive for
             SARS-CoV-2

        Exclusion Criteria:

          -  None
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
