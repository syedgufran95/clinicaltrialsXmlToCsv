  <primary_outcome>
    <measure>Proportions (active vs. placebo) of subjects with progressive respiratory insufficiency.</measure>
    <time_frame>Day 14</time_frame>
    <description>Progressive respiratory insufficiency defined as a ≥ 2-tier increase in respiratory support methods required to maintain satisfactory oxygenation (SpO2 ≥ 93%), using the 6-tier hierarchical scale of respiratory support methods</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Proportions (active vs. placebo) of subjects experiencing treatment-emergent adverse events</measure>
    <time_frame>Day 14</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Time to clinical recovery</measure>
    <time_frame>Day 14</time_frame>
    <description>Clinical recovery defined as time from randomization to disease resolution status on an 8 point Clinical Status scale</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportions (active vs. placebo) of subjects with respiratory failure or death</measure>
    <time_frame>Day 28</time_frame>
  </secondary_outcome>
