  <brief_title>Pso-Vac: Educational Needs re Vaccines for Biologic Patients With Psoriasis</brief_title>
  <official_title>Education on Vaccinations for Psoriatic Patients Who Are on Biologics</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Dermatrials Research</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
