  <arm_group>
    <arm_group_label>Arm 1</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Hydroxychloroquine loading dose (400 mg BID for 2 days) followed by 200 mg BID for 4 days plus standard of care</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Arm 2</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>Hydroxychloroquine loading dose (400 mg BID) alone plus standard of care</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Arm 3</arm_group_label>
    <arm_group_type>Active Comparator</arm_group_type>
    <description>Chloroquine 500 mg BID for 5 days plus standard of care</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Arm 4</arm_group_label>
    <arm_group_type>Placebo Comparator</arm_group_type>
    <description>Standard of care plus placebo (cannot be treated with hydroxychloroquine or chloroquine)</description>
  </arm_group>
