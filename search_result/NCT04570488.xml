  <brief_title>Predicting Favorable Outcomes in Hospitalized Covid-19 Patients</brief_title>
  <official_title>Predicting Favorable Outcomes in Hospitalized Covid-19 Patients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>NYU Langone Health</agency>
      <agency_class>Other</agency_class>
