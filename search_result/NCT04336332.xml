  <brief_title>Randomized Comparison of Combination Azithromycin and Hydroxychloroquine vs. Hydroxychloroquine Alone for the Treatment of Confirmed COVID-19</brief_title>
  <official_title>Randomized Comparison of Combination Azithromycin and Hydroxychloroquine vs. Hydroxychloroquine Alone for the Treatment of Confirmed COVID-19</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Rutgers, The State University of New Jersey</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Rutgers, The State University of New Jersey</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
